 }
 
 function wireEvents(){
     $('li.previous a').click(function(){
         
          var curr = $('div[active="true"]');
        if ($(curr).data('pos') ==='first') return;
       
        var prev = curr.prev('div.wizard-step');
       
        
        if (prev){
             
              curr.removeAttr('active');
              curr.attr('active','false');
              
              curr.hide();
              prev.attr('active','true');
              prev.show();
        }
         
       
     });
     
      $('li.next a').click(function(){
         
        var curr = $('div[active="true"]');
        if ($(curr).data('pos') ==='last') return;
       
        var next = curr.next('div.wizard-step');
       
        
        if (next){
             
              curr.removeAttr('active');
              curr.attr('active','false');
              
              curr.hide();
              next.attr('active','true');
              next.show();
        }
         
         
     });
     
 }
 
